<div class="row">
  <div class="col s12 m12">
    <h1> Questions </h1>
    <h2> Logged in as <%=@current_user.type%>: <%=@current_user.first_name%> </h2>
    <div class="divider"></div>
  </div> 
</div>
<div class="row">
  <%= render @lessons%>
</div>
<div class="row">
  <%= render @questions, current_user_type: @current_user.type %>
</div>

<% if @current_user.type == 'Teacher' %>
  <div class="fixed-action-btn" style="bottom: 45px; right: 24px;">
    <a class="btn-floating btn-large red">
      <i class="large material-icons">add</i>
    </a>
     <ul>
      <li><a class="btn-floating red" href="<%= new_question_path %>"><i class="material-icons">assessment</i></i></a></li>
      <li><a class="btn-floating yellow darken-1" href="<%= new_lesson_path %>"><i class="material-icons">play_circle_outline</i></a></li>
    </ul>
  </div>
<% end %>

