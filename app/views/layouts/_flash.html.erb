
<% flash.each do |key, value| %>
  <div class="row b-flash">
    <div class="col s12 m12">
      <div class="card-panel teal">
        <div class="white-text b-flash_content">
          <%= value%>
          <a class="waves-effect waves-light btn b-flash_close">Close</a>
        </div>
      </div>
    </div>
  </div>
<% end %>